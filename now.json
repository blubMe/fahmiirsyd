{
    "version": 2,
    "name": "fahmiirsyd",
    "builds": [
      {
        "src": "package.json",
        "use": "npm run build",
        "config": { "distDir": "dist" }
      }
    ],
    "alias": ["fahmiirsyd.now.sh"],
    "routes": [
      {
        "src": "/static",
        "dest": "/404.html",
        "status": 404
      },
      {
        "src": "/static/",
        "dest": "/404.html",
        "status": 404
      }
    ]
  }